<script type="text/javascript">
    var index="";
    //iframe高度自适应 2
    function iframeAutoHeight(){
        var iframe=document.getElementById("myiframe");
        if(navigator.userAgent.indexOf("MSIE")>0||navigator.userAgent.indexOf("rv:11")>0||navigator.userAgent.indexOf("Firefox")>0){
            iframe.height=iframe.contentWindow.document.body.scrollHeight;
        }else{
            iframe.height=iframe.contentWindow.document.documentElement.scrollHeight;
        }
        var iframe1=document.getElementById("myiframe1");
        if(navigator.userAgent.indexOf("MSIE")>0||navigator.userAgent.indexOf("rv:11")>0||navigator.userAgent.indexOf("Firefox")>0){
            iframe1.height=iframe1.contentWindow.document.body.scrollHeight;
        }else{
            iframe1.height=iframe1.contentWindow.document.documentElement.scrollHeight;
        }
    }


    function changeFrameHeight(){
        var ifm= document.getElementById("myiframe");
        ifm.height=document.documentElement.clientHeight-78;
    }
    window.onresize=function(){ iframeAutoHeight();}
    $(function(){
        iframeAutoHeight();
    });

    window.addEventListener('message', function(messageEvent) {
        $("#datas tbody").html("");
        var infoList = messageEvent.data;
        for(var i in infoList) {
            var tbBody ="<tr><td>";
            tbBody += infoList[i].title == null ? '</td><td>' : infoList[i].title+"</td><td>";
            tbBody += infoList[i].otherTitle == null ? '</td><td>' : infoList[i].otherTitle+"</td><td>";
            tbBody += infoList[i].seriesNumber == null ? '</td><td>' : infoList[i].seriesNumber+"</td><td>";
            tbBody += infoList[i].seriesTitle == null ? '</td><td>' : infoList[i].seriesTitle+"</td><td>";
            tbBody += infoList[i].author == null ? '</td><td>' : infoList[i].author+"</td><td>";
            tbBody += infoList[i].pissn == null ? '</td><td>' : infoList[i].pissn+"</td><td>" ;
            tbBody += infoList[i].eissn == null ? '</td><td>' : infoList[i].eissn+"</td><td>" ;
            tbBody += infoList[i].pisbn == null ? '</td><td>' : infoList[i].pisbn+"</td><td>" ;
            tbBody += infoList[i].eisbn == null ? '</td><td>' : infoList[i].eisbn+"</td><td>" ;
            tbBody += infoList[i].coverId == null ? '</td><td>' : infoList[i].coverId+"</td><td>" ;
            tbBody += infoList[i].kindId == null ? '</td><td>' : infoList[i].kindId+"</td><td>" ;
            tbBody += infoList[i].coilingPeriod == null ? '</td><td>' : infoList[i].coilingPeriod+"</td><td>" ;
            tbBody += infoList[i].subject == null ? '</td><td>' : infoList[i].subject+"</td><td>" ;
            tbBody += infoList[i].publisher == null ? '</td><td>' : infoList[i].publisher+"</td><td>" ;
            tbBody += infoList[i].country == null ? '</td><td>' : infoList[i].country+"</td><td>" ;
            tbBody += infoList[i].pubYear == null ? '</td><td>' : infoList[i].pubYear+"</td><td>" ;
            tbBody += infoList[i].foundedYear == null ? '</td><td>' : infoList[i].foundedYear+"</td><td>" ;
            tbBody += infoList[i].language == null ? '</td><td>' : infoList[i].language+"</td><td>" ;
            tbBody += infoList[i].pubFeq == null ? '</td><td>' : infoList[i].pubFeq+"</td><td>" ;
            tbBody += infoList[i].Intro == null ? '</td><td>' : infoList[i].Intro+"</td><td>" ;
            tbBody += infoList[i].classiNumber == null ? '</td><td>' : infoList[i].classiNumber+"</td><td>" ;
            tbBody += infoList[i].url == null ? '</td><td>' : infoList[i].url+"</td><td>" ;
            tbBody += infoList[i].carrier == null ? '</td><td>' : infoList[i].carrier+"</td><td>" ;
            tbBody += infoList[i].type == null ? '</td><td>' : infoList[i].type+"</td><td>" ;
            tbBody += infoList[i].pubPattern == null ? '</td><td>' : infoList[i].pubPattern+"</td><td>" ;
            tbBody += infoList[i].pubStatus == null ? '</td>' : infoList[i].pubStatus+"</td></tr>";
            $("#datas").append(tbBody);
        }
        layer.open({
            type: 1,
            title:'详细信息',
            area: ['1400px',''],
            shadeClose: false,
            content: $('#add_administrator_style'),
        });
    }, false);
</script>
