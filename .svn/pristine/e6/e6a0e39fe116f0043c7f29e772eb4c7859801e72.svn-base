<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<!--suppress ALL-->
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:th="http://www.thymeleaf.org"
      xmlns:layout="http://www.ultraq.net.nz/thymeleaf/layout">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="Cache-Control" content="no-siteapp"/>
    <link href="../new/assets/css/bootstrap.min.css" rel="stylesheet"/>
    <link rel="stylesheet" href="../new/css/style.css"/>
    <link href="../new/assets/css/codemirror.css" rel="stylesheet">

    <link rel="stylesheet" href="../ace/components/select2/dist/css/select2.css" />
    <link rel="stylesheet" href="../new/assets/css/ace.min.css" class="ace-main-stylesheet" id="main-ace-style" />
    <link rel="stylesheet" href="../new/font/css/font-awesome.min.css"/>
    <!--[if lte IE 8]>
    <link rel="stylesheet" href="../new/assets/css/ace-ie.min.css"/>
    <![endif]-->
    <script src="../new/js/jquery-1.9.1.min.js"></script>
    <script src="../new/assets/js/bootstrap.min.js"></script>
    <script src="../new/assets/js/typeahead-bs2.min.js"></script>
    <script src="../new/assets/layer/layer.js" type="text/javascript"></script>
    <script src="../new/assets/laydate/laydate.js" type="text/javascript"></script>
    <script src="../new/js/H-ui.js" type="text/javascript"></script>
    <script src="../ace/components/jquery-validation/dist/jquery.validate.js"></script>
    <script type="text/javascript" src="../business/common.js"></script>
    <script type="text/javascript" src="../business/base.js"></script>
    <title>资源详情</title>
</head>

<body>
<div class="margin clearfix">
    <div class="article_style">
        <div class="add_content" id="form-article-add">
            <form id="validation-form">
                <div class="col-sm-6">
                    <ul>
                        <li class="clearfix Mandatory">
                            <label class="label_name">题名:</label>
                            <span class="formControls col-sm-6"th:text="${info.title}"> </span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">其他题名:</label>
                            <span class="formControls col-sm-6"th:text="${info.otherTitle}"> </span>
                        </li>
                        <!--<li class="clearfix Mandatory">-->
                            <!--<label class="label_name">分辑号:</label>-->
                            <!--<span class="formControls col-sm-6"th:text="${info.seriesNumber}"> </span>-->
                        <!--</li>-->
                        <!--<li class="clearfix Mandatory">-->
                            <!--<label class="label_name">分辑名:</label>-->
                            <!--<span class="formControls col-sm-6"th:text="${info.seriesTitle}"> </span>-->
                        <!--</li>-->
                        <li class="clearfix Mandatory">
                            <label class="label_name">责任者:</label>
                            <span class="formControls col-sm-6"th:text="${info.author}"> </span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">EISSN:</label>
                            <span class="formControls col-sm-6" th:text="${info.eissn}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">EISBN:</label>
                            <span class="formControls col-sm-6" th:text="${info.eisbn}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">PISSN:</label>
                            <span class="formControls col-sm-6" th:text="${info.pissn}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">PISBN:</label>
                            <span class="formControls col-sm-6" th:text="${info.pisbn}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">套名:</label>
                            <span class="formControls col-sm-6" th:text="${info.coverId}"></span>
                        </li>
                        <!--<li class="clearfix Mandatory">-->
                            <!--<label class="label_name">种类:</label>-->
                            <!--<span class="formControls col-sm-6" th:text="${info.kindId}"></span>-->
                        <!--</li>-->
                        <li class="clearfix Mandatory">
                            <label class="label_name">卷期:</label>
                            <span class="formControls col-sm-6" th:text="${info.coilingPeriod}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">学科:</label>
                            <span class="formControls col-sm-6" th:text="${info.subject}"></span>
                        </li>
                    </ul>
                </div>
                <div class="col-sm-6">
                    <ul>
                        <li class="clearfix Mandatory">
                            <label class="label_name">出版社:</label>
                            <span class="formControls col-sm-6" th:text="${info.publisher}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">国别:</label>
                            <span class="formControls col-sm-6" th:text="${info.country}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">出版年:</label>
                            <span class="formControls col-sm-6" th:text="${info.pubYear}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">创刊年:</label>
                            <span class="formControls col-sm-6" th:text="${info.foundedYear}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">语种:</label>
                            <span class="formControls col-sm-6" th:text="${info.language}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">出版频率:</label>
                            <span class="formControls col-sm-6" th:text="${info.pubFeq}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">内容简介:</label>
                            <span class="formControls col-sm-6" th:text="${info.Intro}"></span>
                        </li>
                        <!--<li class="clearfix Mandatory">-->
                            <!--<label class="label_name">分类号:</label>-->
                            <!--<span class="formControls col-sm-6" th:text="${info.classiNumber}"></span>-->
                        <!--</li>-->
                        <li class="clearfix Mandatory">
                            <label class="label_name">期刊URL地址:</label>
                            <span class="formControls col-sm-6" th:text="${info.url}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">载体:</label>
                            <span class="formControls col-sm-6" th:text="${info.carrier}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">文献类型:</label>
                            <span class="formControls col-sm-6" th:text="${info.type}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">出版模式:</label>
                            <span class="formControls col-sm-6" th:text="${info.pubPattern}"></span>
                        </li>
                        <li class="clearfix Mandatory">
                            <label class="label_name">出版状态:</label>
                            <span class="formControls col-sm-6" th:text="${info.pubStatus}"></span>
                        </li>
                    </ul>
                </div>

            </form>
            <div class="Button_operation" style="margin-left: 426px" th:switch="${type}">
                <p th:case="'0'">
                    <a class="btn btn-default radius" href="/resourceRecommend/list" type="button"> &nbsp;&nbsp;返回 &nbsp;&nbsp;
                    </a>
                </p>
                <p th:case="'1'">
                    <a class="btn btn-default radius" href="/resourceRecommend/firstTrial" type="button"> &nbsp;&nbsp;返回 &nbsp;&nbsp;
                    </a>
                </p>
                <p th:case="'2'">
                    <a class="btn btn-default radius" href="/resourceRecommend/secondTrial" type="button"> &nbsp;&nbsp;返回 &nbsp;&nbsp;
                    </a>
                </p>
                <p th:case="'3'">
                    <a class="btn btn-default radius" href="/selection/recommended" type="button"> &nbsp;&nbsp;返回 &nbsp;&nbsp;
                    </a>
                </p>
                <p th:case="'4'">
                    <a class="btn btn-default radius" href="/selection/toAuditAssignment" type="button"> &nbsp;&nbsp;返回 &nbsp;&nbsp;
                    </a>
                </p>

                <p th:case="'5'">
                    <a class="btn btn-default radius" href="/dataMaintain/resourcesMaintain" type="button"> &nbsp;&nbsp;返回 &nbsp;&nbsp;
                    </a>
                </p>
            </div>
        </div>
    </div>
</div>
</body>
</html>
