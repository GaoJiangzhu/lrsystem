// out: false
.panel {
    background-color: #fff;
    border-radius: 2px;
    &+& {
        margin-top: 15px;
    }
}

.panel-head {
    padding: 16px;
    border-bottom: 1px solid #E9E9E9;
    font-weight: bold;
    font-size: 16px;
}

.panel-body {
    padding: 15px;
    text-align: center;
    img {
        max-width: 100%;
    }
}